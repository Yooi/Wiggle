{
  "name": "wiggle",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:github": "tsc && vite build --mode production",
    "build:all": "npm run build && cd signaling-server && npm install",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "npm run start:all",
    "start:all": "concurrently \"npm run start:signaling\" \"npm run preview\"",
    "start:signaling": "cd signaling-server && npm start",
    "install:all": "npm install && cd signaling-server && npm install",
    "deploy:vercel": "vercel --prod",
    "deploy:gh-pages": "npm run build:github && gh-pages -d dist"
  },
  "dependencies": {
    "@chainsafe/libp2p-gossipsub": "^14.1.1",
    "@chainsafe/libp2p-noise": "^16.1.4",
    "@chainsafe/libp2p-yamux": "^7.0.4",
    "@libp2p/circuit-relay-v2": "^3.2.24",
    "@libp2p/identify": "^3.0.39",
    "@libp2p/mplex": "^11.0.47",
    "@libp2p/webrtc": "^5.2.24",
    "@libp2p/websockets": "^9.2.19",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.15",
    "@tailwindcss/postcss": "^4.1.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "libp2p": "^2.10.0",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "simple-peer": "^9.11.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uint8arrays": "^5.1.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/simple-peer": "^9.11.8",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^7.6.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  }
}
